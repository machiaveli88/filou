{
  "name": "vilicando-core",
  "private": true,
  "scripts": {
    "build": "run-p build:*",
    "build:antd": "cd antd && yarn build",
    "build:core": "cd core && yarn build",
    "dev": "yarn dev:basic",
    "dev:basic": "cd examples/basic && yarn dev",
    "dev:env": "cd examples/env && yarn dev",
    "dev:ssr": "cd examples/ssr && yarn dev",
    "dev:theming": "cd examples/theming && yarn dev",
    "dev:translation": "cd examples/translation && yarn dev",
    "pub": "run-p pub:*",
    "pub:antd": "cd antd && npm publish",
    "pub:core": "cd core && npm publish",
    "up": "run-p up:*",
    "up:antd": "cd antd && yarn up",
    "up:core": "cd core && yarn up",
    "up:example-basic": "cd examples/basic && yarn up",
    "up:example-env": "cd examples/env && yarn up",
    "up:example-ssr": "cd examples/ssr && yarn up",
    "up:example-theming": "cd examples/theming && yarn up",
    "up:example-translation": "cd examples/translation && yarn up",
    "watch": "run-p watch:*",
    "watch:antd": "cd antd && yarn build:watch",
    "watch:core": "cd core && yarn build:watch"
  },
  "workspaces": [
    "antd",
    "core",
    "examples/*"
  ],
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
