{
  "name": "vilicando-core",
  "private": true,
  "scripts": {
    "build": "yarn build:core & run-p build:*",
    "build:antd": "cd antd && yarn build",
    "build:core": "cd core && yarn build",
    "build:hasura": "cd hasura && yarn build",
    "dev": "yarn dev:basic",
    "dev:antd": "cd examples/antd && yarn dev",
    "dev:basic": "cd examples/basic && yarn dev",
    "dev:env": "cd examples/env && yarn dev",
    "dev:hasura": "cd examples/hasura && yarn dev",
    "dev:ssr": "cd examples/ssr && yarn dev",
    "dev:theming": "cd examples/theming && yarn dev",
    "dev:translation": "cd examples/translation && yarn dev",
    "pub": "run-p pub:*",
    "pub:antd": "cd antd && npm publish",
    "pub:core": "cd core && npm publish",
    "theme": "cd antd && yarn theme",
    "up": "yarn upgrade-interactive --latest && yarn check",
    "watch": "run-p watch:*",
    "watch:antd": "cd antd && yarn build:watch",
    "watch:core": "cd core && yarn build:watch",
    "watch:hasura": "cd hasura && yarn build:watch"
  },
  "workspaces": [
    "antd",
    "core",
    "hasura",
    "examples/*"
  ],
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
